// user defined functions
function show_calender () {
    if(!dropdown_flag)
    {
       if(flag) 
       {
            var currentTime = new Date();
            // returns the month (from 0 to 11)
            var current_month = currentTime.getMonth() + 1;
            // returns the year (four digits)
            var current_year = currentTime.getFullYear();
            // set calender properties
            picker = new Pikaday(
            {
                field: document.getElementById(parameters['calender_div_id']),
                format: parameters['date_format']
            });
            calender_base_url = parameters['base_url'];
            date_format = parameters['date_format'];
            dropdown_flag = true;
            document.getElementById(parameters['calender_month_div_id']).style.display = 'block';
            document.getElementById(parameters['calender_year_div_id']).style.display = 'block';
            picker.setMaxDate(currentDate);
            if(typeof(parameters['selected_date']) != "undefined" && parameters['selected_date'] !== null) {
                picker.setDate(parameters['selected_date']);
            }
            
            picker.show();

            var month = document.getElementById(parameters['calender_month_div_id']);

            month.addEventListener("change", function() {
                calender_widget();
            });
            var year = document.getElementById(parameters['calender_year_div_id']);
 
            year.addEventListener("change", function() {
                calender_widget();
            });
       }
    }
    else
    {
        document.getElementById(parameters['calender_month_div_id']).style.display = 'block';
        document.getElementById(parameters['calender_year_div_id']).style.display = 'block';
        document.getElementById("j_calender_div_id").style.display = 'block';
    }
}

function calender_widget() {
    var month = document.getElementById(parameters['calender_month_div_id']);
    month = month.value;
    month=month-1;
    var year = document.getElementById(parameters['calender_year_div_id']);
    year =year.value;
    picker.setDate(new Date(year, month, current_day));
    picker.show();
}

if(flag) {
    show_calender();
}

function open_url(ele) {
	var edition = document.getElementById(parameters['epaper_edition']);
	var calender_type =  parameters['calender_type'];

	if (edition !== null && edition !== undefined)
	{
		if (date_format !== null)
		{
			var selected_edition = edition.options[edition.selectedIndex].value;
			var date_string = new Date(picker.calendars[0].year, picker.calendars[0].month,ele.getAttribute("data-pika-day"));
			date_string=date_string.format(date_format);
			var no_wrapper = location.search.split('?')[1] ? location.search.split('?')[1] : '';

			if ( no_wrapper == '' )
			{
				window.location = calender_base_url+'E-Paper/'+selected_edition+'/'+date_string;
			}
			else
			{
				window.location = calender_base_url+'E-Paper/'+selected_edition+'/'+date_string+'?'+no_wrapper;
			}

			fadeout_calender();
		}
		else
		{
			alert('Please set the date format');
		}
	}
	else if (calender_type !== null && calender_type !== undefined && calender_type == 'archives')
	{
		if (date_format !== null)
		{
			var date_string = new Date(picker.calendars[0].year, picker.calendars[0].month,ele.getAttribute("data-pika-day"));
			date_string = date_string.format(date_format);
			window.location = calender_base_url+'archives/'+date_string;
			fadeout_calender();
		}
		else
		{
			alert('Please set the date format');
		}
	}
	else
	{
		alert('Please set the E paper edition');
	}
}

